{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cross-env NODE_ENV=development PORT=5001 tsx server/index.ts",
    "dev:frontend": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build -c vite.server.config.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:pm2": "pm2 start ecosystem.config.cjs",
    "stop:pm2": "pm2 stop neet-prep",
    "restart:pm2": "pm2 restart neet-prep",
    "logs:pm2": "pm2 logs neet-prep",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "seed:full": "tsx server/seeds/run-all.ts",
    "lint": "tsc --noEmit",
    "audit:prod": "npm audit --production",
    "analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "test:booking": "tsx client/src/components/mentors/bookingUtils.spec.ts",
    "enhance-visualizations": "tsx server/scripts/enhance-chapter-visualizations.ts",
    "docker:build": "docker build -t neet-prep .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "health": "curl -f http://localhost:5001/api/health || exit 1"
  },
